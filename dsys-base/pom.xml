<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.dsys</groupId>
        <artifactId>dsys</artifactId>
        <version>0.0.1-SNAPSHOT</version><!-- lookup parent from repository -->
        <relativePath/>
    </parent>
    <groupId>com.dsys</groupId>
    <artifactId>dsys-base</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>dsys-base</name>
    <description>dsys-base</description>
    <dependencies>
        <dependency>
            <groupId>com.dsys</groupId>
            <artifactId>dsys-api</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.dsys</groupId>
            <artifactId>dsys-common-util</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
            <exclusions>
                <exclusion>
                    <groupId>org.junit.vintage</groupId>
                    <artifactId>junit-vintage-engine</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>io.projectreactor</groupId>
            <artifactId>reactor-test</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>com.dsys</groupId>
            <artifactId>dsys-service-util</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.dsys</groupId>
            <artifactId>dsys-web-util</artifactId>
            <version>0.0.1-SHAPSHOT</version>
            <scope>compile</scope>
        </dependency>
